package work01;
import java.util.Scanner;
public class Work02 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in= new Scanner(System.in);
		int data1=in.nextInt();
		int data2=in.nextInt();
		var ops=in.nextLine();
		
		
		if (ops=="+")
			System.out.println(data1+data2);
		else if (ops=="-")
		{
			System.out.println(data1-data2);
		}
		else if (ops=="*")
		{
			System.out.println(data1*data2);
		}
		else if (ops=="/")
		{
			System.out.println(data1/data2);
		}
		else ;
		
	}

}
